  "version": "0.2.0",
  "configurations": [
    {
      "name": "trn_admin_flutter",
      "request": "launch",
      "type": "dart",
      "toolArgs": [
        "--dart-define",
        "flavor=dev",
        "--web-port",
        "8080"
      ]
    },
    {
      "name": "trn_admin_flutter (WASM)",
      "request": "launch",
      "type": "dart",
      "toolArgs": [
        "--wasm",
        "--dart-define",
        "flavor=dev",
        "--web-port",
        "8080"
      ]
    },
    {
      "name": "trn_admin_flutter (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "toolArgs": [
        "--dart-define",
        "flavor=dev",
        "--web-port",
        "8080"
      ]
    },
    {
      "name": "trn_admin_flutter (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "toolArgs": [
        "--dart-define",
        "flavor=prod",
        "--web-port",
        "8080"
      ]
    }
  ]
}